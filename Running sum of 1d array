//https://leetcode.com/problems/running-sum-of-1d-array/
class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        int sum=0;
    int n=nums.size();
        vector<int> v(nums.size());
        for(int i=0;i<n;i++)
        { sum+=nums[i];
            v[i]=sum;
        } return v;
    }
};
